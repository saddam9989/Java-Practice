package com.singleinheritance.java;

public class Animal {
	
	public void eat() {
		System.out.println("Animals are eating");
	}

}

 class Dog extends Animal{
	 
	 public void bark() {
		 System.out.println("Dog is barking");
	 }
	
}

 class TestSingleInheritance{
	
	 public static void main(String[] args) {
		
		 Dog dog=new Dog();
		 
		 dog.bark();
		 dog.eat();
	}
 }